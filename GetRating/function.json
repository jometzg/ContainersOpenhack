{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "rating",
      "databaseName": "RatingDatabase",
      "collectionName": "RatingCollection",
      "connectionStringSetting": "jjratingsaccount_DOCUMENTDB",
      "userId": "{Query.userId}",
      "PartitionKey": "{Query.partitionKeyValue}"
    }
  ]
}
